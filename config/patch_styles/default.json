{
  "indentationSpacesPerModifier" : 2,
  //Outer wrapper brackets
  "newLineAfterOuterOpenerBracket" : true,
  "newLineAfterOuterCloserBracket" : true,
  "indentationInOuterBrackets" : true,
  //Operation set wrapper brackets (Starbound)
  "newLineAfterOperationSetOpenerBracket" : true,
  "newLineAfterOperationSetCloserBracket" : false,
  "newLineAfterOperationSetCloserBracketComma" : true,
  "indentationInOperationSetBrackets" : true,
  //Operation wrapper brackets
  "newLineAfterOperationOpenerBracket" : true,
  "newLineAfterOperationCloserBracket" : false,
  "newLineAfterOperationCloserBracketComma" : true,
  "indentationInOperationBrackets" : true,
  //Operation contents
  "newLineAfterOperationSegment" : true,
  "spaceBeforeOperationColon" : true,
  "spaceAfterOperationColon" : true
}

